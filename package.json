{
  "name": "@jcwillox/semantic-release-config",
  "version": "0.0.0-dev",
  "description": "A semantic-release config with improved defaults and configurable through environment variables",
  "author": "Josh Willox <joshwillox@gmail.com> (https://github.com/jcwillox/)",
  "license": "MIT",
  "repository": "jcwillox/semantic-release-config",
  "keywords": [
    "semantic-release"
  ],
  "type": "module",
  "scripts": {
    "semantic-release": "node ./dist/cli.js",
    "dev": "tsup --watch",
    "build": "tsup",
    "typecheck": "tsc",
    "lint": "eslint . --cache --max-warnings=0 --ext js,cjs,mjs,jsx,ts,tsx",
    "lint:fix": "pnpm run lint --fix",
    "format": "prettier --cache --write .",
    "format:check": "prettier --cache --check ."
  },
  "dependencies": {
    "@semantic-release/error": "^4.0.0",
    "@semantic-release/git": "^10.0.1",
    "conventional-changelog-conventionalcommits": "^8.0.0",
    "destr": "^2.0.3",
    "execa": "^9.4.0",
    "lodash": "^4.17.21",
    "semantic-release": "^24.1.2"
  },
  "devDependencies": {
    "@tsconfig/node20": "20.1.4",
    "@types/lodash": "4.17.9",
    "@types/node": "20.16.10",
    "@types/semantic-release__error": "3.0.3",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "eslint": "8.57.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.6.3",
    "eslint-plugin-import": "npm:eslint-plugin-i@2.29.1",
    "eslint-plugin-only-warn": "1.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "prettier": "3.3.3",
    "tsup": "8.3.0",
    "typescript": "5.6.2"
  },
  "bin": "dist/cli.js",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./*": {
      "import": {
        "types": "./dist/*.d.ts",
        "default": "./dist/*.js"
      },
      "require": {
        "types": "./dist/*.d.cts",
        "default": "./dist/*.cjs"
      }
    },
    "./*/*": {
      "import": {
        "types": "./dist/*/*.d.ts",
        "default": "./dist/*/*.js"
      },
      "require": {
        "types": "./dist/*/*.d.cts",
        "default": "./dist/*/*.cjs"
      }
    },
    "./changelog": {
      "import": {
        "types": "./dist/changelog.d.ts",
        "default": "./dist/changelog.js"
      },
      "require": {
        "types": "./dist/changelog.d.ts",
        "default": "./dist/changelog.js"
      }
    },
    "./exec-cmd": {
      "import": {
        "types": "./dist/exec-cmd.d.ts",
        "default": "./dist/exec-cmd.js"
      },
      "require": {
        "types": "./dist/exec-cmd.d.ts",
        "default": "./dist/exec-cmd.js"
      }
    },
    "./force-publish": {
      "import": {
        "types": "./dist/force-publish.d.ts",
        "default": "./dist/force-publish.js"
      },
      "require": {
        "types": "./dist/force-publish.d.ts",
        "default": "./dist/force-publish.js"
      }
    }
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "packageManager": "pnpm@9.11.0",
  "engines": {
    "node": "^20 || ^22"
  }
}
